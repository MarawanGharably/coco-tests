{
  "name": "coco-cms-frontend",
  "version": "0.2.0",
  "description": "coco-cms-frontend",
  "main": "index.js",
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "pre-commit": "lint-staged",
    "lint": "npx eslint --ext js,jsx .",
    "lint-fix": "npx eslint --ext js,jsx --fix .",
    "test": "jest",
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --env dev --mode development",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-dev": "webpack -p --config ./webpack/webpack.config.js --env dev --progress --display-error-details --mode development",
    "build-feature": "webpack -p --config ./webpack/webpack.config.js --env feature --progress --display-error-details --mode development",
    "build-beta": "webpack -p --config ./webpack/webpack.config.js --env beta --progress --display-error-details --mode development",
    "build-prod": "webpack -p --config ./webpack/webpack.config.js --env prod --progress --display-error-details --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/ObsessVR/coco-cms-frontend.git"
  },
  "author": "Obsess, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.12.5",
    "@babel/runtime": "^7.12.5",
    "@emotion/babel-preset-css-prop": "^10.2.1",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "css-loader": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^6.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.1",
    "raw-loader": "^2.0.0",
